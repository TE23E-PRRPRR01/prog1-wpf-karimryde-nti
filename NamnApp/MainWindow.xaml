<Window x:Class="NamnApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NamnApp"
        mc:Ignorable="d"
        Title="Namn o sånt" Height="450" Width="800"
        SizeToContent="WidthAndHeight">
    <StackPanel Background="#EEE">
        <Label Margin="10" FontSize="30">Initialer</Label>
        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Förnamn: </Label>
            <TextBox Margin="10" Width="150" Name="txbFörnamn"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Efternamn: </Label>
            <TextBox Margin="10" Width="150" Name="txbEfternamn"></TextBox>
        </StackPanel>
        <Button Click="KlickKör" Margin="10" Padding="5" Width="150">Kör</Button>
        <StackPanel Orientation="Horizontal">
            <Label Margin="10" Width="70">Resultat: </Label>
            <TextBox Margin="10" Width="150" Name="txbResultat"></TextBox>
        </StackPanel>
    </StackPanel>
</Window>
